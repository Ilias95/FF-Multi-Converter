#!/usr/bin/env python2

if __name__ == '__main__':
    import sys
    try:
        from ffmulticonverter import ffmulticonverter
    except ImportError:
        # ffmulticonverter is not installed
        # load module from the same folder
        import ffmulticonverter

    ffmulticonverter.main(sys.argv[1:])

